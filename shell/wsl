.wslconfig

[wsl2]
# kernel=D:\\Wsl\\bzImage
guiApplications = false
vmIdleTimeout = -1
memory = 5G
swap = 10G
networkingMode = mirrored
autoProxy = true

[experimental]
hostAddressLoopback = true
autoMemoryReclaim = dropcache


/etc/wsl.conf

[interop]
enabled = true
appendWindowsPath = false

[boot]
systemd = true

[user] #可以忽略这个配置
default = user_name
